 public int maximumWealth(int[][] accounts) {
       
        int res=Integer.MIN_VALUE;
        for(int[] acc:accounts)
        {
             int sum=0;
            for(int val:acc)
            {
                sum+=val;
            }
            System.out.println(sum);
            if(sum>res)
            {
                res=sum;
            }
        }
        return res;
    }
