import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        ArrayList<ArrayList> list=new ArrayList<>();
        
        for(int i=0;i<3;i++)
        {
            list.add(new ArrayList<>());
        }
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                list.get(i).add(in.nextLine());
            }
        }
        System.out.println(list);
    }
}
